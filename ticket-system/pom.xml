        <artifactId>mobot-modules</artifactId>
        <version>1.0.0</version>
    </parent>

    <name>ticket-system</name>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>9</source>
                    <target>9</target>
                </configuration>
            </plugin>
        </plugins>
    </build>


    <dependencies>
        <dependency>
            <groupId>com.pixelservices.mobot</groupId>
            <artifactId>mobot-api</artifactId>
        </dependency>
    </dependencies>
</project>
